[FUNCTION eqArmor]
local.moddex=<armor_getModdex <argn>>
obj=<argn>

//serv.log aplicando moddex -<dlocal.moddex> em <hval <obj.dispid>>
moddex -= <local.moddex>

//Aplica resistência a danos especiais
IF (<obj.type>==t_armor) || (<obj.type>==t_armor_leather) || (<obj.type>==t_shield)
    resfire += <obj.morex>
    rescold += <obj.morey>
    respoison += <obj.morez>
    resenergy += <obj.morem>
ENDIF

[FUNCTION unequipArmor]
local.moddex=<armor_getModdex <argn>>
obj=<argn>

//serv.log aplicando moddex <dlocal.moddex> em <hval <obj.dispid>>
moddex += <local.moddex>

//Retira resistência a danos especiais
IF (<obj.type>==t_armor) || (<obj.type>==t_armor_leather) || (<obj.type>==t_shield)
    resfire -= <obj.morex>
    rescold -= <obj.morey>
    respoison -= <obj.morez>
    resenergy -= <obj.morem>
ENDIF

