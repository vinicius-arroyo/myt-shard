[ITEMDEF i_mry_foodeffect]
NAME=Comida com Efeito
ID=0993
TYPE=t_eq_script
LAYER=layer_special
WEIGHT=0

on=@Equip
if (<more1>==1)
    //empadao de coelho
    src.sysmessageyellow Voce se sente POTENTE
    src.emotered coca virilha e bate pe no chao repetidas vezes
    remove
    return 0
elif (<more1>==2)
    //gemada mu sun
    sfx <src.SEX 042C/031B>
    src.sysmessagegreen Isso tem um gosto horrivel, mas eh revigorante!
    remove
    return 0
elif (<more1>==3)
    //bagos de alkron e racho de luna
    sfx <src.SEX 042C/031B>
    src.sysmessagegreen Isso tem um gosto horrivel!!!
    remove
    return 0
elif (<more1>==4)
    //linguica laveque
    src.sysmessagered VOCE SE EXCITA
    src.emotered estremece
    src.anim 20
    remove
    return 0
    IF (<src.0body> c_man)
        src.sfx 608
        src.emotered Um volume protubera na regiao pubiana
        remove
        return 0        
    ELSE
        src.sfx 1340
        src.emotered ensopada
        src.emotered na regiao pubiana
        remove
        return 0    
    ENDIF
elif (<more1>==5)
    //picanha de gorgon
    sfx <src.SEX 044A/0338>
    emotegreen grita
    src.anim 33
    src.sysmessage voce se sente mais vigoroso
    remove
    return 0
endif
remove
return 1


