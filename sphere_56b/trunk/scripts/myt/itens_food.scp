[ITEMDEF i_rotulador]
ID=01031
NAME=Rotulador de Alimentos
CATEGORY=Myt - items by professions
SUBSECTION=Cook
DESCRIPTION=Rotulador de Alimentos e Bebidas
WEIGHT=1.0
VALUE=80
TYPE=t_buy_misc
RESOURCES=1 i_pen_and_ink, 1 i_bobina_papel
SKILLMAKE=Tinkering 700

ON=@CREATE
COLOR=08ac
ON=@DCLICK
if (<topobj> != <src>)
    src.sysmessageyellow Coloque rotulador na bolsa antes de usa-lo.
    return 1
endif

target Selecione o que deseja rotular
return 1

ON=@TARGON_ITEM
if (<src.targ.topobj> != <src>)
    src.sysmessageyellow Coloque isso na bolsa para rotular.
    else
    if (<src.targ.type>==t_food)
        src.sysmessageyellow Digite um nome para o rotulo:
        src.promptconsole rotulo_comida
    elif (<src.targ.type>==t_recipient) && (<src.targ.more2>>=10) && (<src.targ.more2><=14)
        src.sysmessageyellow Digite um nome para o rotulo:
        src.promptconsole rotulo_bebida
    endif
endif
return 1

//*****************************************************************************
// rotulo_comida
//*****************************************************************************
[FUNCTION rotulo_comida]
src.targ.name <targ.name>, <args>
src.targ.update

//*****************************************************************************
// rotulo_bebida
//*****************************************************************************
[FUNCTION rotulo_bebida]
src.targ.name <targ.name>, <args>
src.targ.update

