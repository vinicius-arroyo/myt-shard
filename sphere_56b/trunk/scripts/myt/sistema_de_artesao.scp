if (<eval <SERV.DEF.p<LOCAL.p>Amt4>>>0)
	ctag.res4=<SERV.ITEMDEF.<SERV.DEF.p<LOCAL.p>Res4>.name>
	ctag.amt4=<SERV.DEF.p<LOCAL.p>Amt4>
