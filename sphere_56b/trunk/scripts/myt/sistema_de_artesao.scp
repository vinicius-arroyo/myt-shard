if (<eval <SERV.DEF.p<LOCAL.p>Amt3>>>0)
	ctag.res3=<SERV.ITEMDEF.<SERV.DEF.p<LOCAL.p>Res3>.name>
	ctag.amt3=<SERV.DEF.p<LOCAL.p>Amt3>
else
	ctag.res3=
	ctag.amt3=
