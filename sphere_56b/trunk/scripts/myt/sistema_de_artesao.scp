dialog d_ar_pro 

//*****************************************************************************
// art_transcribe( proj_item, dif )
//*****************************************************************************
[FUNCTION art_transcribe]

if (<src.inscription> < <argv[1]>)
 src.sysmessagered Essa transcricao esta alem da sua habilidade.
 return 1
endif

//if (!<src.restest 1 i_tinta>)
// src.sysmessagered Voce precisa ter tinta.
// return 1
//endif

obj=<src.findid.i_faz_item.uid>
obj.cleartags
obj.tag.delay=5
obj.tag.skill=inscription
obj.tag.dif=<eval <argv[1]>/10>
obj.tag.anim=19
obj.tag.sfx=019
obj.tag.res1=i_documento_branco
obj.tag.nam1=documento em branco
obj.tag.amt1=1
obj.tag.item=<argv[0]>
obj.tag.emote=transcrevendo
obj.tag.needtype=t_escrivaninha
obj.tag.need=escrivaninha
//obj.tag.f_end=art_gastaTinta
src.fazitem

return 1
//*****************************************************************************
// art_copyKey
//*****************************************************************************
[FUNCTION art_copyKey]
sysmessage Felecione a chave a copiar:
targetf _art_copyKey

[FUNCTION _art_copyKey]
obj=<argo>
if (<obj.type>!=t_key)
 sysmessageyellow Isso nao eh uma chave
 return 1
elif (<obj.topobj>!=<uid>)
 sysmessageyellow A chave deve estar em sua posse.
 return 1
elif (!<obj.more1>)
 sysmessageyellow Esta chave esta em branco.
 return 1
elif ((<obj.baseid>==i_key_copper) && (<tinkering> < 55.0)) || ((<obj.baseid>==i_key_iron) && (<tinkering> < 65.0)) || ((<obj.baseid>==i_key_gold) && (<tinkering> < 70.0))
 sysmessageyellow Voce nao tem habilidade para copiar uma <obj.name>.
 return 1
endif
LOCAL.key=<obj.uid>
say Fazer chave.
obj=<src.findid.i_faz_item.uid>
obj.cleartags
obj.tag.delay=12
obj.tag.skill=tinkering
obj.tag.dif=60
obj.tag.anim=19
obj.tag.sfx=02e2
obj.tag.res1=i_barra_metal
obj.tag.nam1=barra de metal
obj.tag.amt1=1
obj.tag.emote=limando
obj.tag.neartype=t_bancada
obj.tag.nearname=bancada
obj.tag.item=<UID.<LOCAL.key>.baseid>
obj.tag.more1=<UID.<LOCAL.key>.more1>
obj.tag.abort=1
src.fazitem

return 1

//*****************************************************************************
// art_gastaTinta
//*****************************************************************************
[FUNCTION art_gastaTinta]
if (!<argv[1]>)
	OBJ=src.findid(i_tinta).uid
	OBJ.more1 -= 1
	OBJ.name=tinta (<eval <OBJ.more1>>)
	if (<OBJ.more1>==0)
		OBJ.remove
		sysmessageyellow Sua tinta acabou!!!
	endif
endif
return 1

