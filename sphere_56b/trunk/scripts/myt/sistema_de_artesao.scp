//*****************************************************************************
// art_prevProj
//*****************************************************************************
[FUNCTION art_prevProj]
src.ctag.ar_page=<src.ctag0.ar_page>-1
if (!<src.ctag0.ar_page>)
	src.ctag.ar_page=31
endif
LOCAL.m=<eval 2@(<src.ctag.ar_page>-1)>

WHILE !(<LOCAL.m>&<src.ctag0.ar_projs>)
	src.ctag.ar_page=<src.ctag0.ar_page>-1
	if (!<src.ctag.ar_page>)
		src.ctag.ar_page=31
	endif
	LOCAL.m=<eval 2@(<src.ctag.ar_page>-1)>
ENDWHILE

if (<src.ctag.ar_page>==1)
	src.dialog d_ar_pro_main
else
	art_openBook
endif
return 1

//*****************************************************************************
// art_nextProj
//*****************************************************************************
[FUNCTION art_nextProj]
src.ctag.ar_page=<src.ctag0.ar_page>+1
if (<src.ctag.ar_page> > 01f)
	src.ctag.ar_page=1
endif
LOCAL.m=<eval 2@(<src.ctag.ar_page>-1)>

WHILE !(<LOCAL.m>&<src.ctag0.ar_projs>)
	src.ctag.ar_page=<src.ctag0.ar_page>+1
	if (<src.ctag.ar_page> > 01f)
		src.ctag.ar_page=1
	endif
	LOCAL.m=<eval 2@(<src.ctag.ar_page>-1)>
ENDWHILE

if (<src.ctag.ar_page>==1)
	src.dialog d_ar_pro_main
else
	art_openBook
endif

return 1


//*****************************************************************************
// art_openBook
//*****************************************************************************
[FUNCTION art_openBook]
LOCAL.p=<ctag.ar_page>
ctag.name=<SERV.ITEMDEF.<SERV.DEF.p<LOCAL.p>Item>.name>
