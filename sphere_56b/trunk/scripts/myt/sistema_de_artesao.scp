//*****************************************************************************
// t_ar_projeto
//*****************************************************************************
[TYPEDEF t_ar_projeto]
ON=@DCLICK
TARGET Selecione o livro onde incluir:
return 1

ON=@TARGON_CHAR
src.sysmessagered Selecione um livro de projetos!
return 1

ON=@TARGON_GROUND
src.sysmessagered Selecione um livro de projetos!
return 1

ON=@TARGON_ITEM
if (<src.targ.BaseID>==i_livro_projetos)
 if (<src.targ.more1>&<more1>)
  src.sysmessageyellow O livro ja contem este projeto
  return 1
 endif
 src.targ.more1=(<src.targ.more1>|<more1>)
 src.sysmessagegreen <name> adicionado ao livro.
 remove
 return 1
else
src.sysmessagered Selecione um livro de projetos!
return 1

//*****************************************************************************
// t_ar_livro
//*****************************************************************************
[TYPEDEF t_ar_livro]
ON=@DCLICK
if (!<more2>)
 name=<name> (<src.tag.name>)
 more2=1
endif
src.ctag.ar_page=1
src.ctag.ar_projs=<more1>
src.dialog d_ar_pro_main
return 1

